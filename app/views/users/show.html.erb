<div>
  <h3><%= @user.name %></h3>
  <%= image_tag @user.get_profile_image(100,100) %>
  <% if @user == current_user %>
    <p><%= link_to "プロフィール編集", edit_user_path(@user) %></p>
  <% end %>
</div>

<% @books.each do |book| %>
<div>
  <p><%= image_tag post_image.user.get_profile_image(100,100) %></p>
  <p>Title</p>
  <%= book.title %>
  <p>説明</p>
  <%= book.body %>
</div>
<% end %>
